<!DOCTYPE html>
<html lang="ja">
<head>
  <meta charset="utf-8">
  <title>500 Internal Server Error</title>
  <style type="text/css">
    body {
      position:relative;
      font-size:28px;
      font-family:sans-serif;
      line-height:1.6;
      margin:0;
      padding:0;
      color:#323232;
      background:#eee url(data:image/png;base64,);
    }
    #header {
      height: 68px;
      line-height: 68px;
      margin: 0;
      padding-left: 40px;
      border-bottom: 2px solid #49A10F;
      font-size:28px;
      font-weight:bold;
      text-shadow: 0 -2px 0 rgba(0, 0, 0, 0.2);
      color:#fff;
      background: #5AC800;
      background: -webkit-gradient(linear, left top, left bottom, from(#81E52E), to(#5AC800));
    }
    #body {
      width:960px;
      margin:0 auto;
    }
    .title {
      clear:both;
      padding:15px 20px;
      margin-top:1.5em;
      background: #828282;
      color:#fff;
      font-size:32px;
      text-shadow: 0 -2px 2px #555;
      border-radius:12px;
    }
    #content {
      min-height:367px;
      padding-right:280px;
      background-repeat: no-repeat;
      background-position: right center;
    }
    #exit-button {
      position: fixed;
      top: 620px;
      left: 0;
     display: block;
      -webkit-box-shadow: 0 3px 10px rgba(0, 0, 0, 0.6);
      height: 100px;
      line-height: 100px;
      padding: 0 50px 0 80px;
      color: white;
      text-decoration:none;
      background: url(data:image/png;base64,) no-repeat 36px 32px, -webkit-gradient(linear, left top, left bottom, from(#5A5A5A), to(#242424));
      -webkit-border-radius: 0 80px 0 0;
       z-index: 2;
    }
  </style>
  <script src="/js/jquery-1.7.2.min.js"></script>
  <script type="text/javascript">
  if(window.wiiuSystemSetting){
    var region = wiiuSystemSetting.getRegion();
    var lang = wiiuSystemSetting.getLanguage();
    if(region.error){region.code = 'JPN';}
    if(lang.error){lang.code = 'ja';}
  }else{
    var region = {'code' : 'JPN'};
    var lang = {'code' : 'ja'};
  }
  var trans = {
